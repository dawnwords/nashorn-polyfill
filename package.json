{
  "name": "nashorn-polyfill",
  "version": "0.0.1",
  "description": "Polyfill for Nashorn Script Engine",
  "main": "index.js",
  "repository": "https://github.com/shendepu/nashorn-polyfill.git",
  "author": "Jimmy Shen <shendepu@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "build:polyfill": "node node_modules/webpack/bin/webpack.js --config ./config/webpack.config.dev.js",
    "build:polyfill:prod": "node node_modules/webpack/bin/webpack.js --config ./config/webpack.config.prod.js",
    "build:server-polyfill": "node node_modules/gulp/bin/gulp.js scripts",
    "build:dev": "npm run clean && npm run build:server-polyfill && npm run build:polyfill",
    "build:prod": "npm run clean && npm run build:server-polyfill && npm run build:polyfill:prod"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "core-js": "^2.4.1",
    "url-search-params": "^0.6.1",
    "webpack": "2.1.0-beta.25"
  }
}
